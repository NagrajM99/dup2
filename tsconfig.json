{
  "compilerOptions": {
    "target": "ES2017",
    "module": "UMD",
    "outDir": "./",
    "rootDir": "./",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "declaration": false,
    "removeComments": true
  },
  "include": ["index.ts"],
  "exclude": ["node_modules"]
}
